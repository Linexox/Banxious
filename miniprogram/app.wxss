/** app.wxss **/
/* Note: @font-face via local path is not supported in Miniprogram wxss on some platforms.
   We will load it via wx.loadFontFace in app.js for better compatibility.
*/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  font-family: 'XiangJiaoFont', sans-serif; /* Apply globally */
} 
